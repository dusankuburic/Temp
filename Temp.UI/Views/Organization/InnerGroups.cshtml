@model Temp.Application.Organizations.GetInnerGroups.Response

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="col-lg-8">
    @if(Model != null)
    {
    <h1 class="text-center">Groups inside @Model.Name</h1>
        <table class="table table-striped text-center">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th colspan="3">Options</th>
            </tr>
   
                @foreach (var group in Model.Groups)
                {
                <tr>
                <td>@group.Id</td>
                <td>@group.Name</td>
                </tr>
                }      
        </table>
    }
</div>